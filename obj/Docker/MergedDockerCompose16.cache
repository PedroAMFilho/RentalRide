name: dockercompose9641259380221003895
services:
  pgadmin:
    container_name: pgadmin
    depends_on:
      rentalride.database:
        condition: service_started
        required: true
    environment:
      PGADMIN_DEFAULT_EMAIL: user@localhost.com
      PGADMIN_DEFAULT_PASSWORD: admin
    image: dpage/pgadmin4:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "8080"
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: C:\Projects\RentalRideProject\RentalRide\data\pgadmin
        target: /var/lib/pgadmin
        bind:
          create_host_path: true
  rentalride.api:
    build:
      context: C:\Projects\RentalRideProject\RentalRide\RentalRide.Api
      dockerfile: Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: rentalrideapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        protocol: tcp
      - mode: ingress
        target: 443
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\hisqu\AppData\Roaming/Microsoft/UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\hisqu\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  rentalride.database:
    container_name: rentalride.database
    environment:
      POSTGRES_DB: rentalride
      POSTGRES_PASSWORD: admin
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Projects\RentalRideProject\RentalRide\.containers\rentalride-db
        target: /var/lib/postgresql
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose9641259380221003895_default